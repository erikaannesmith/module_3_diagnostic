<%= @stations.count %> Results

<div class="stations">
  <% @stations.each do |station| %>
    <ul class="station">
      <li class="name">Name: <%= station.name %></li>
      <li class="address">Address: <%= station.address %></li>
      <li class="fuel_type">Fuel Type: <%= station.fuel_type %></li>
      <li class="distance">Distance: <%= station.distance %></li>
      <li class="access_times">Access Times: <%= station.access_times %></li>
    </ul>
  <% end %>
</div>